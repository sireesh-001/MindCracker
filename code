    private void b2ActionPerformed(java.awt.event.ActionEvent evt) {                                   

         
        
         j=tf1.getText();
        String un2=tf1.getText();
String pass2=tf2.getText();
String un=un2.toLowerCase();
String pass=pass2.toLowerCase();

if (un.isEmpty()||pass.isEmpty())
JOptionPane.showMessageDialog(this,"Name or password not Entered");
else
{
    try
{
Class.forName("java.sql.DriverManager");
Connection con = (Connection)
DriverManager.getConnection
("jdbc:mysql://localhost:3306/sireesh", "root","root");
Statement stmt = (Statement) con.createStatement();
String query="SELECT * FROM iit WHERE user_name ='"+un+"'&& password='"+pass+"';";
ResultSet rs=stmt.executeQuery(query);
int Found=0;
 while(rs.next())
{
String un1 = rs.getString("user_name");
String password = rs.getString("password");
if(un1.equals(un)&&password.equals(pass)){
JOptionPane.showMessageDialog(null,"sucessfully logged in \n Click OK to continue!!!");
lh56.setText(tf1.getText());

mock_test k= new mock_test();
k.name(tf1.getText());
k.setVisible(true);


}
else
{
    JOptionPane.showMessageDialog(null,"Incorrect password or username");
}
Found++;
}
if (Found==0)

JOptionPane.showMessageDialog(this,"Sorry! No such Account");


}
catch (Exception e)
{
JOptionPane.showMessageDialog(this, e.getMessage());
}
}


    }                                  
 
