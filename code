    private void jButton3KeyPressed(java.awt.event.KeyEvent evt) {                                    
if(evt.getKeyCode()==KeyEvent.VK_ENTER){
    String n=tf3.getText();
        String un=tf4.getText();
        String school=tf58.getText();
        String  em=tf6.getText();
        String pa=tf7.getText();
        String npa=tf8.getText();
        String state = (String)cb2.getSelectedItem();
        if(pa.equals(npa)) {
            try {
                Class.forName("java.sql.DriverManager");
                Connection con = (Connection)
                        DriverManager.getConnection("jdbc:mysql://localhost:3306/sireesh",
                        "root", "root");
                Statement stmt = (Statement) con.createStatement();
                String x="insert into iit values('"+un+"','"+em+"','"+pa+"','"+npa+"','"+school+"','"+state+"','"+n+"');";
                stmt.executeUpdate(x);
                con.close();
                stmt.close();
                JOptionPane.showMessageDialog(null," login-please");
                JD1_.dispose();
            } catch(Exception e) {

                JOptionPane.showMessageDialog(this, e.getMessage());
            }  } else {
            JOptionPane.showMessageDialog(null,"RE-ENTER CONFIRM PASSWORD ");
            }

}
    }                                   

    private void jButton4KeyPressed(java.awt.event.KeyEvent evt) {                                    
        if(evt.getKeyCode()==KeyEvent.VK_ENTER){
            if(rd1.isSelected()||rd2.isSelected()||rd3.isSelected()==true) { if(rd1.isSelected()){
            jd3_.setVisible(true);
            lb7.setText("mathematics");} else if(rd2.isSelected()){
                jd3_.setVisible(true);
                lb7.setText("physics");}

            else if(rd3.isSelected()){
                jd3_.setVisible(true);
                lb7.setText("chemistry");}
        jd2_.dispose();} else if(rd1.isSelected()||rd2.isSelected()||rd3.isSelected()==false) {
            JOptionPane.showMessageDialog(null,"please select any subject");
        }
        }
    }                                   

    private void jButton6KeyPressed(java.awt.event.KeyEvent evt) {                                    
        if(evt.getKeyCode()==KeyEvent.VK_ENTER){
           if(rd5.isSelected()||rd6.isSelected()==true){
            jd3_.dispose();
            main_.setVisible(true);
            lb1.setText(lb7.getText());
           
            if(rd5.isSelected())
                lb8.setText("question bank-01");
            else if(rd6.isSelected())
                lb8.setText("question bank-02");} else if(rd5.isSelected()||rd6.isSelected()==false) {
                    JOptionPane.showMessageDialog(null,"please select a Question bank");
                }
        }
    }                                   

