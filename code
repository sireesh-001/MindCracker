    private void jLabel39KeyPressed(java.awt.event.KeyEvent evt) {                                    
        if(evt.getKeyCode()==KeyEvent.VK_ENTER){
            if(rd5.isSelected()||rd6.isSelected()==true){
                jd3_.dispose();
                main_.setVisible(true);
                lb1.setText(lb7.getText());
               
                if(rd5.isSelected())
                    lb8.setText("question bank-01");
                else if(rd6.isSelected())
                    lb8.setText("question bank-02");} else if(rd5.isSelected()||rd6.isSelected()==false) {
                        JOptionPane.showMessageDialog(null,"please select a Question bank");
                    }
        }
}                                   

