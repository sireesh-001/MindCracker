    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        if(rd1.isSelected()||rd2.isSelected()||rd3.isSelected()==true) { if(rd1.isSelected()){
            jd3_.setVisible(true);
            lb7.setText("mathematics");} else if(rd2.isSelected()){
                jd3_.setVisible(true);
                lb7.setText("physics");}

            else if(rd3.isSelected()){
                jd3_.setVisible(true);
                lb7.setText("chemistry");}
        jd2_.dispose();} else if(rd1.isSelected()||rd2.isSelected()||rd3.isSelected()==false) {
