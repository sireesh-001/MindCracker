    private void ip_WindowOpened(java.awt.event.WindowEvent evt) {                                 
       b10.setOpaque(false);
      b10.setContentAreaFilled(false); 
      b11.setOpaque(false);
      b11.setContentAreaFilled(false); 
     b10.setBorderPainted(false);
     b11.setBorderPainted(false);
      String a=JOptionPane.showInputDialog(null,"username");
      try {
                    Class.forName("java.sql.DriverManager");
                    Connection con = (Connection)
                            DriverManager.getConnection("jdbc:mysql://localhost:3306/sireesh", "root","root");
                    Statement stmt = (Statement) con.createStatement();
                    String query="SELECT first_name FROM iit WHERE user_name ='"+a+"';";
                    ResultSet rs=stmt.executeQuery(query);
                    while(rs.next())

                    {
                       String un1 = rs.getString("first_name");
                       hk55.setText(un1);
                   
                   
                       

                    }
                     con.close();
                    stmt.close();


                } catch (Exception e) {
                    JOptionPane.showMessageDialog(this, e.getMessage());

                }
           
      

      
    }                                

