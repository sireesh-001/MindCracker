public static String getIP1()
{
try{
InetAddress ownIP=InetAddress.getLocalHost();
return(ownIP.getHostAddress());
}
catch (Exception e){
return(e.getMessage());
}
}
 public static String getUser()
{
String userid;
try{
Class.forName("java.sql.Driver");
Connection con = (Connection)
DriverManager.getConnection("jdbc:mysql://localhost:3306/sireesh", "root","root");
String ip = Mains1.getIP1();
Statement stmt4 = con.createStatement();
String sql4 = "select * from logbook where ip='"+ip+"';";
ResultSet rs4 = stmt4.executeQuery(sql4);
rs4.next();
userid=rs4.getString("userid");
return userid;
}
catch(Exception e){
JOptionPane.showMessageDialog(null,e.getMessage());
return null;
}
}
