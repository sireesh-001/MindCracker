String bar(int x){
    String query1 = null;
    if(j02.getText().equals("mathematics")&&lb8.getText().equals("question bank-01"))
            query1="SELECT *FROM math_q1_a where sno='"+x+"';";
        else if(j02.getText().equals("mathematics")&&lb8.getText().equals("question bank-02"))
            query1="SELECT *FROM math_q2_a where sno='"+x+"';";
        else if(j02.getText().equals("physics")&&lb8.getText().equals("question bank-01"))
            query1="SELECT *FROM phy_q1_a where sno='"+x+"';";
        else if(j02.getText().equals("physics")&&lb8.getText().equals("question bank-02"))
            query1="SELECT *FROM phy_q2_a where sno='"+x+"';";
        else if(j02.getText().equals("chemistry")&&lb8.getText().equals("question bank-01"))
              query1="SELECT *FROM chem_q1_a where sno='"+x+"';";
        else if(j02.getText().equals("chemistry")&&lb8.getText().equals("question bank-02"))
              query1="SELECT *FROM chem_q2_a where sno='"+x+"';";
    return(query1);

}
